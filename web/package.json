{
  "name": "book-store-app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Dmitriy Dubson <d.dubson@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "BOOKS_API_URI=http://localhost:8080 ./node_modules/.bin/parcel src/index.html",
    "build": "./node_modules/.bin/parcel build src/index.html",
    "test": "tslint 'src/**/*.ts' && jest",
    "lint": "tslint 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "jest": "^23.5.0",
    "parcel": "^1.9.7",
    "react-test-renderer": "^16.4.2",
    "serve": "^10.0.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@types/axios": "^0.14.0",
    "@types/material-ui": "^0.21.5",
    "@types/react": "^16.4.12",
    "@types/react-dom": "^16.0.7",
    "axios": "^0.18.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "enzyme-react-16-adapter-setup"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/"
    ]
  }
